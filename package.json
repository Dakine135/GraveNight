{
  "name": "gravenight",
  "version": "1.0.0",
  "description": "Javascript multiplayer game",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "build": "npx webpack --config webpack.config.js",
    "share": "lt --port 3033",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dakine135/GraveNight.git"
  },
  "keywords": [
    "multiplayer",
    "node",
    "javascript",
    "topdown",
    "2D",
    "CustomEngine"
  ],
  "author": "Graham Shroyer, Alexander Scott, Dustin Welborn",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Dakine135/GraveNight/issues"
  },
  "homepage": "https://github.com/Dakine135/GraveNight#readme",
  "dependencies": {
    "express": "^4.17.1",
    "reload": "^3.0.1",
    "socket.io": "^2.2.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "public/js/clientBundle.js",
      "public/js/workers.js"
    ],
    "delay": "2500",
    "watch": [
      "index.html",
      "public",
      "server.js",
      "server",
      "shared",
      "clientSource"
    ],
    "events": {
      "restart": "npm run build",
      "start": "npm run build"
    }
  }
}
